{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Alias stack for refcoach-dev (dev)",
  "Resources": {
    "ServerlessAliasLogGroup": {
      "Type": "AWS::Logs::LogGroup",
      "Properties": {
        "LogGroupName": "/serverless/refcoach-dev-dev",
        "RetentionInDays": 7
      }
    }
  },
  "Outputs": {
    "ServerlessAliasName": {
      "Description": "Alias the stack represents.",
      "Value": "dev"
    },
    "ServerlessAliasLogGroup": {
      "Description": "Log group for alias.",
      "Value": {
        "Ref": "ServerlessAliasLogGroup"
      },
      "Export": {
        "Name": "refcoach-dev-dev-LogGroup"
      }
    }
  }
}